{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "sort-keys": [2, "asc", { "caseSensitive": true, "natural": true, "minKeys": 2 }],
    "sort-vars": [2, { "ignoreCase": true }]
  }
}
